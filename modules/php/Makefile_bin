NAME = php

SRC =	PHPModule.cpp \
			main.cpp \
			../../trunk/apisources/Response.cpp \
			../../trunk/apisources/Request.cpp \
			../../trunk/apisources/RequestHeader.cpp \
			../../trunk/apisources/ResponseHeader.cpp \
			../../trunk/HTTPParser.cpp \
			../../trunk/apisources/Body.cpp

OBJ = $(SRC:.cpp=.o)

CXXFLAGS =
LDFLAGS =

$(NAME): $(OBJ)
	$(CXX) -o $(NAME) $(OBJ) $(LDFLAGS)

all: $(NAME)

clean:
	$(RM) $(OBJ)

fclean: clean
	$(RM) $(NAME)

re: fclean all
